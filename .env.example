# Group Moderation (Default Settings - Override per group)
#â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

# Anti-link: Remove messages with links
ANTI_LINK=false
ANTI_LINK_ACTION=delete
#   Actions: warn, delete, kick, ban

# Anti-invite: Block group invite links
ANTI_INVITE=false

# Anti-bad words: Filter profanity
ANTI_BAD_WORDS=false
BAD_WORDS=
#   Comma-separated list

# Anti-spam: Duplicate message detection
ANTI_SPAM_MESSAGES=false

# Anti-toxic: AI-based toxicity detection (requires API)
ANTI_TOXIC=false
TOXIC_THRESHOLD=0.7

# Anti-NSFW: Block NSFW images (requires API)
ANTI_NSFW=false

# Anti-foreign: Block messages in foreign languages
ANTI_FOREIGN=false
ALLOWED_LANGUAGES=en

