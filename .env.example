UNKNOWN_COMMAND_REPLY=false

# Message for unknown commands (supports {prefix} placeholder)
UNKNOWN_COMMAND_MESSAGE=â“ Unknown command. Type {prefix}help for available commands.

# Show command suggestions for typos
SUGGEST_COMMANDS=true

