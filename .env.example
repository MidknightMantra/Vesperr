#  5. CONNECTION SETTINGS
#â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

# Display QR code in terminal
#   Set to false for headless/server deployments using pairing code
PRINT_QR=true

# Use pairing code instead of QR
#   Requires PHONE_NUMBER to be set
PAIRING_CODE=false

# Phone number for pairing code (with country code, no + or spaces)
#   Example: 14155551234
PHONE_NUMBER=

# Browser identification [name, browser, version]
#   Appears in WhatsApp linked devices list
BROWSER=WhatsApp Bot,Chrome,125.0.0

# Sync full chat history on first connect
#   Warning: Can be slow and use significant bandwidth
SYNC_FULL_HISTORY=false

# Appear online when connected
MARK_ONLINE=true

# Auto-reconnect on disconnection
RETRY_ON_DISCONNECT=true

# Maximum reconnection attempts before giving up
MAX_RETRIES=15

# Delay between reconnection attempts (milliseconds)
RETRY_DELAY=5000

# Connection timeout (milliseconds)
CONNECTION_TIMEOUT=60000

#â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
#  6. RESPONSE BEHAVIOR
#â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

# Respond in group chats
RESPOND_IN_GROUPS=true

# Respond in private/DM chats
RESPOND_IN_PRIVATE=true

# Respond in channels/newsletters
RESPOND_IN_CHANNELS=false

# Respond in broadcast lists
RESPOND_IN_BROADCASTS=false

# Respond to status/story replies
RESPOND_TO_STATUS=false

