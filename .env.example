# Messages per hour (per user)
RATE_LIMIT_PER_HOUR=200

# Messages per day (global/bot total)
RATE_LIMIT_PER_DAY=1000

# Default command cooldown (milliseconds)
DEFAULT_COOLDOWN=3000

# Premium user cooldown multiplier (0.5 = half cooldown)
PREMIUM_COOLDOWN_MULTIPLIER=0.5

